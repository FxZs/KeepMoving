<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cn.room.activity">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity android:name=".RoomActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.fx.keepmoving.MainActivity"></activity>
        <activity android:name="com.com.tworoom.TwoRoomActivity"/>
        <activity android:name="com.com.threeday.ThreeDataActivity"/>
        <activity android:name="com.com.fourday.FourActivity"/>
        <activity android:name="com.com.fiveday.FivewActivity"/>
        <activity android:name="com.com.fiveday.FiveActivity"/>
        <activity android:name="com.sixday.SixActivity"/>
        <activity android:name="com.elevenday.ElevenActivity"/>
        <activity android:name="com.sixday.RetorActivity"/>
        <activity android:name="newday.com.MainActivityNewDay"/>
        <activity android:name="newfourday.com.NewFourDayActivity"/>
    </application>

</manifest>